# copy8 @rg0, 53
# copy8 @rg1, 0
# copy8 [@rg1], @rg0
# writes @rg0, 1

# 0x08 0x70 0x00 0x35
# 0x08 0x80 0x00 0x00 
# 0x0C 0x87 0x00 0x00
# 0x00 0x70 0x00 0x01


byts = bytearray([0x08, 0x70, 0x00, 0x35, 0x08, 0x80, 0x00, 0x00, 0x0C, 0x87,
                  0x00, 0x00, 0x00, 0x70, 0x00, 0x01])

with open('./bytes', 'wb') as file:
    file.write(byts)
